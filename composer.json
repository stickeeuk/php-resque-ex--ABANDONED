{
    "name": "stickee/php-resque-ex",
    "type": "library",
    "description": "Redis backed library for creating background jobs and processing them later. PHP port based on resque for Ruby.",
    "keywords": [
        "job",
        "background",
        "redis",
        "resque",
        "queue",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin.meredith@stickee.co.uk",
            "homepage": "https://www.stickee.co.uk/"
        },
        {
            "name": "Wan Chen",
            "email": "kami@kamisama.me",
            "homepage": "http://www.kamisama.me"
        },
        {
            "name": "Chris Boulton",
            "email": "chris@bigcommerce.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Resque\\": [
                "src",
                "tests/src"
            ]
        }
    },
    "support": {
        "email": "comparison-dev@stickee.co.uk",
        "issues": "https://github.com/stickeeuk/php-resque-ex/issues",
        "source": "https://github.com/stickeeuk/php-resque-ex"
    },
    "bin": [
        "bin/resque"
    ],
    "conflict": {
        "chrisboulton/php-resque": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0",
        "ext-redis": "*",
        "ext-pcntl": "*",
        "monolog/monolog": ">=1.2.0",
        "kamisama/monolog-init": ">=0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "phpro/grumphp": "^0.11.5",
        "friendsofphp/php-cs-fixer": "~1",
        "jakub-onderka/php-parallel-lint": "^0.9.2"
    }
}
